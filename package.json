{
  "name": "@goy/utils",
  "version": "0.0.1",
  "description": "GoyFE team's utils",
  "homepage": "https://github.com/goy-fe/utils",
  "bugs": {
    "url": "https://github.com/goy-fe/utils/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/goy-fe/utils.git"
  },
  "license": "MIT",
  "author": "ntnyq <ntnyq13@gmail.com> (https://ntnyq.com)",
  "main": "dist/goy-utils.cjs.js",
  "module": "dist/goy-utils.esm.js",
  "browser": "dist/goy-utils.min.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup --config scripts/rollup-config.js",
    "postbuild": "npm run build:docs",
    "build:docs": "jsdoc -c jsdoc.json",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "new": "plop",
    "release": "standard-version",
    "pretest": "npm run lint:fix",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "package.json": "sort-package-json"
  },
  "browserslist": [
    "> 1%",
    "last 2 version",
    "not IE <= 10"
  ],
  "prettier": "prettier-config-goy",
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@types/jest": "^26.0.10",
    "babel-jest": "^26.3.0",
    "docdash": "^1.2.0",
    "eslint": "^7.7.0",
    "eslint-config-goy": "^0.2.3",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "jsdoc": "^3.6.5",
    "lint-staged": "^10.2.13",
    "plop": "^2.7.4",
    "prettier": "^2.1.1",
    "prettier-config-goy": "^1.1.0",
    "rollup": "^2.26.8",
    "rollup-plugin-terser": "^7.0.1",
    "sort-package-json": "^1.44.0",
    "standard-version": "^9.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
