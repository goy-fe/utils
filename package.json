{
  "name": "@goy/utils",
  "version": "0.0.1",
  "description": "GoyFE team's utils",
  "homepage": "https://github.com/goy-fe/utils",
  "bugs": {
    "url": "https://github.com/goy-fe/utils/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/goy-fe/utils.git"
  },
  "license": "MIT",
  "author": "ntnyq <ntnyq13@gmail.com> (https://ntnyq.com)",
  "main": "dist/goy-utils.cjs.js",
  "module": "dist/goy-utils.esm.js",
  "browser": "dist/goy-utils.min.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup --config scripts/rollup-config.js",
    "postbuild": "npm run build:docs",
    "build:docs": "jsdoc -c jsdoc.json",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "new": "plop",
    "release": "standard-version",
    "pretest": "npm run lint:fix",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "package.json": [
      "sort-package-json",
      "git add"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 version",
    "not IE <= 10"
  ],
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@rollup/plugin-buble": "^0.21.1",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.3.1",
    "@types/jest": "^25.2.1",
    "babel-jest": "^25.3.0",
    "docdash": "^1.2.0",
    "eslint": "^6.8.0",
    "eslint-config-goy": "^0.2.1",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "jsdoc": "^3.6.4",
    "lint-staged": "^10.1.3",
    "plop": "^2.6.0",
    "prettier": "^2.0.4",
    "rollup": "^2.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "sort-package-json": "^1.41.0",
    "standard-version": "^7.1.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
